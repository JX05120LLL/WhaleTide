date.getFullYear(), \"\\u5E74\").concat(date.getMonth() + 1, \"\\u6708\").concat(date.getDate(), \"\\u65E5\");\n    });\n\n    // 激活的菜单项\n    var activeMenu = computed(function () {\n      if (route.name === 'UserCenter') {\n        return 'profile';\n      }\n      return route.path.split('/').pop();\n    });\n\n    // 用户相关信息\n    var userInfo = reactive({\n      balance: '0.00',\n      coupons: 0,\n      points: 0,\n      favorites: 0\n    });\n\n    // 各状态订单数量\n    var orderCount = reactive({\n      unpaid: 0,\n      unshipped: 0,\n      shipped: 0,\n      completed: 0,\n      afterSale: 0\n    });\n\n    // 最近订单\n    var recentOrders = ref([{\n      id: 10001,\n      orderNumber: 'WT20230511001',\n      createTime: '2023-05-11 10:30:22',\n      amount: 299.00,\n      status: 'unpaid',\n      statusText: '待付款'\n    }, {\n      id: 10002,\n      orderNumber: 'WT20230510002',\n      createTime: '2023-05-10 15:20:18',\n      amount: 4578.00,\n      status: 'shipped',\n      statusText: '待收货'\n    }, {\n      id: 10003,\n      orderNumber: 'WT20230508003',\n      createTime: '2023-05-08 09:15:32',\n      amount: 128.50,\n      status: 'completed',\n      statusText: '已完成'\n    }]);\n\n    // 获取状态对应的类型\n    var getStatusType = function getStatusType(status) {\n      switch (status) {\n        case 'unpaid':\n          return 'danger';\n        case 'unshipped':\n          return 'warning';\n        case 'shipped':\n          return '';\n        case 'completed':\n          return 'success';\n        case 'afterSale':\n          return 'info';\n        default:\n          return '';\n      }\n    };\n\n    // 加载用户中心数据\n    var loadUserCenterData = /*#__PURE__*/function () {\n      var _ref = _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              try {\n                // 在实际应用中，此处应该调用API获取数据\n                // const response = await request.get('/user/center/data')\n                // userInfo.balance = response.data.balance\n                // userInfo.coupons = response.data.coupons\n                // userInfo.points = response.data.points\n                // userInfo.favorites = response.data.favorites\n\n                // 模拟数据\n                userInfo.balance = '1,258.50';\n                userInfo.coupons = 3;\n                userInfo.points = 520;\n                userInfo.favorites = 8;\n                orderCount.unpaid = 1;\n                orderCount.unshipped = 0;\n                orderCount.shipped = 1;\n                orderCount.completed = 5;\n                orderCount.afterSale = 0;\n              } catch (error) {\n                console.error('获取用户中心数据失败:', error);\n              }\n            case 1:\n            case \"end\":\n              return _context.stop();\n          }\n        }, _callee);\n      }));\n      return function loadUserCenterData() {\n        return _ref.apply(this, arguments);\n      };\n    }();\n\n    // 菜单选择处理\n    var handleMenuSelect = function handleMenuSelect(index) {\n      if (route.path !== \"/user/\".concat(index)) {\n        router.push(\"/user/\".concat(index));\n      }\n    };\n\n    // 去购物\n    var goShopping = function goShopping() {\n      router.push('/');\n    };\n\n    // 查看所有订单\n    var goToOrders = function goToOrders(status) {\n      router.push({\n        path: '/user/orders',\n        query: status ? {\n          status: status\n        } : {}\n      });\n    };\n\n    // 查看订单详情\n    var viewOrderDetail = function viewOrderDetail(orderId) {\n      router.push(\"/order/\".concat(orderId));\n    };\n    onMounted(function () {\n      loadUserCenterData();\n    });\n    return {\n      user: user,\n      currentDate: currentDate,\n      activeMenu: activeMenu,\n      userInfo: userInfo,\n      orderCount: orderCount,\n      recentOrders: recentOrders,\n      handleM