{"ast":null,"code":"import { ref, reactive, onMounted } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useStore } from 'vuex';\nimport { ElMessage } from 'element-plus';\nimport { ShoppingCart, Goods, Present, MagicStick, Discount, Collection, Tickets } from '@element-plus/icons-vue';\nimport AppHeader from '@/components/Header.vue';\nimport AppFooter from '@/components/Footer.vue';\nimport request from '@/utils/request';\nexport default {\n  name: 'HomePage',\n  components: {\n    AppHeader,\n    AppFooter,\n    ShoppingCart,\n    Goods,\n    Present,\n    MagicStick,\n    Discount,\n    Collection,\n    Tickets\n  },\n  setup() {\n    const router = useRouter();\n    const store = useStore();\n\n    // 轮播图数据\n    const banners = ref([{\n      id: 1,\n      title: '新品上市',\n      subtitle: '2023春季新品，限时8折优惠',\n      image: 'https://via.placeholder.com/1200x400?text=Spring+Sale',\n      link: '/category/new'\n    }, {\n      id: 2,\n      title: '数码专场',\n      subtitle: '精选数码产品，满2000减200',\n      image: 'https://via.placeholder.com/1200x400?text=Digital+Products',\n      link: '/category/digital'\n    }, {\n      id: 3,\n      title: '会员专享',\n      subtitle: '会员专享价，更有积分双倍',\n      image: 'https://via.placeholder.com/1200x400?text=Member+Benefits',\n      link: '/member'\n    }]);\n\n    // 快捷入口\n    const quickEntries = ref([{\n      id: 1,\n      title: '新品发布',\n      icon: 'Present',\n      link: '/category/new'\n    }, {\n      id: 2,\n      title: '热卖商品',\n      icon: 'Goods',\n      link: '/category/hot'\n    }, {\n      id: 3,\n      title: '限时特惠',\n      icon: 'Discount',\n      link: '/category/discount'\n    }, {\n      id: 4,\n      title: '优选好物',\n      icon: 'MagicStick',\n      link: '/category/selected'\n    }, {\n      id: 5,\n      title: '品牌专区',\n      icon: 'Collection',\n      link: '/brands'\n    }]);\n\n    // 热门商品\n    const hotProducts = ref([{\n      id: 101,\n      name: '高性能笔记本电脑',\n      price: 5999,\n      originalPrice: 6799,\n      image: 'https://via.placeholder.com/300x300?text=Laptop',\n      soldCount: 1200,\n      tag: '热销'\n    }, {\n      id: 102,\n      name: '无线蓝牙耳机',\n      price: 299,\n      originalPrice: 399,\n      image: 'https://via.placeholder.com/300x300?text=Headphones',\n      soldCount: 3500,\n      tag: '热销'\n    }, {\n      id: 103,\n      name: '智能手表',\n      price: 1299,\n      originalPrice: 1599,\n      image: 'https://via.placeholder.com/300x300?text=SmartWatch',\n      soldCount: 800,\n      tag: '热销'\n    }, {\n      id: 104,\n      name: '超薄平板电脑',\n      price: 2999,\n      originalPrice: 3599,\n      image: 'https://via.placeholder.com/300x300?text=Tablet',\n      soldCount: 600,\n      tag: '热销'\n    }]);\n\n    // 新品上市\n    const newProducts = ref([{\n      id: 201,\n      name: '智能家居套装',\n      price: 899,\n      originalPrice: null,\n      image: 'https://via.placeholder.com/300x300?text=SmartHome',\n      soldCount: 150,\n      tag: '新品'\n    }, {\n      id: 202,\n      name: '便携显示器',\n      price: 1499,\n      originalPrice: 1699,\n      image: 'https://via.placeholder.com/300x300?text=Monitor',\n      soldCount: 320,\n      tag: '新品'\n    }, {\n      id: 203,\n      name: '机械键盘',\n      price: 399,\n      originalPrice: 499,\n      image: 'https://via.placeholder.com/300x300?text=Keyboard',\n      soldCount: 780,\n      tag: '新品'\n    }, {\n      id: 204,\n      name: '无线充电器',\n      price: 129,\n      originalPrice: 169,\n      image: 'https://via.placeholder.com/300x300?text=Charger',\n      soldCount: 1600,\n      tag: '新品'\n    }]);\n\n    // 精选专题\n    const features = ref([{\n      id: 1,\n      title: '居家办公解决方案',\n      desc: '打造舒适高效的居家办公环境',\n      image: 'https://via.placeholder.com/600x300?text=Home+Office'\n    }, {\n      id: 2,\n      title: '智能生活方式',\n      desc: '用科技改变生活，享受智能化带来的便利',\n      image: 'https://via.placeholder.com/600x300?text=Smart+Life'\n    }]);\n\n    // 品牌专区\n    const brands = ref([{\n      id: 1,\n      name: 'Apple',\n      logo: 'https://via.placeholder.com/200x100?text=Apple'\n    }, {\n      id: 2,\n      name: 'Samsung',\n      logo: 'https://via.placeholder.com/200x100?text=Samsung'\n    }, {\n      id: 3,\n      name: 'Huawei',\n      logo: 'https://via.placeholder.com/200x100?text=Huawei'\n    }, {\n      id: 4,\n      name: 'Xiaomi',\n      logo: 'https://via.placeholder.com/200x100?text=Xiaomi'\n    }, {\n      id: 5,\n      name: 'Sony',\n      logo: 'https://via.placeholder.com/200x100?text=Sony'\n    }, {\n      id: 6,\n      name: 'Dell',\n      logo: 'https://via.placeholder.com/200x100?text=Dell'\n    }]);\n\n    // 跳转到商品详情\n    const goToProduct = id => {\n      router.push(`/product/${id}`);\n    };\n\n    // 跳转到分类页面\n    const goToCategory = category => {\n      router.push(`/category/${category}`);\n    };\n\n    // 跳转到专题页面\n    const goToFeature = id => {\n      router.push(`/feature/${id}`);\n    };\n\n    // 跳转到品牌页面\n    const goToBrand = id => {\n      router.push(`/brand/${id}`);\n    };\n\n    // 添加到购物车\n    const addToCart = product => {\n      store.dispatch('addToCart', {\n        id: product.id,\n        name: product.name,\n        price: product.price,\n        image: product.image,\n        quantity: 1\n      });\n      ElMessage({\n        type: 'success',\n        message: `已将 ${product.name} 加入购物车`\n      });\n    };\n\n    // 获取首页数据\n    const fetchHomeData = async () => {\n      try {\n        // 在实际项目中，应该从API获取数据\n        // const response = await request.get('/home/data')\n        // banners.value = response.data.banners\n        // hotProducts.value = response.data.hotProducts\n        // newProducts.value = response.data.newProducts\n        // 这里使用的是模拟数据\n      } catch (error) {\n        console.error('获取首页数据失败:', error);\n      }\n    };\n    onMounted(() => {\n      fetchHomeData();\n    });\n    return {\n      banners,\n      quickEntries,\n      hotProducts,\n      newProducts,\n      features,\n      brands,\n      goToProduct,\n      goToCategory,\n      goToFeature,\n      goToBrand,\n      addToCart\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","onMounted","useRouter","useStore","ElMessage","ShoppingCart","Goods","Present","MagicStick","Discount","Collection","Tickets","AppHeader","AppFooter","request","name","components","setup","router","store","banners","id","title","subtitle","image","link","quickEntries","icon","hotProducts","price","originalPrice","soldCount","tag","newProducts","features","desc","brands","logo","goToProduct","push","goToCategory","category","goToFeature","goToBrand","addToCart","product","dispatch","quantity","type","message","fetchHomeData","error","console"],"sources":["D:\\code\\project\\wt-client-web\\src\\views\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home-page\">\r\n    <app-header></app-header>\r\n    \r\n    <div class=\"container\">\r\n      <!-- 轮播图 -->\r\n      <div class=\"banner-section\">\r\n        <el-carousel height=\"400px\">\r\n          <el-carousel-item v-for=\"item in banners\" :key=\"item.id\">\r\n            <div class=\"banner-item\" :style=\"{ backgroundImage: `url(${item.image})` }\">\r\n              <div class=\"banner-content\">\r\n                <h2>{{ item.title }}</h2>\r\n                <p>{{ item.subtitle }}</p>\r\n                <el-button type=\"primary\" @click=\"goToProduct(item.link)\">查看详情</el-button>\r\n              </div>\r\n            </div>\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </div>\r\n      \r\n      <!-- 快捷入口 -->\r\n      <div class=\"quick-entry\">\r\n        <div class=\"entry-item\" v-for=\"item in quickEntries\" :key=\"item.id\" @click=\"goToCategory(item.link)\">\r\n          <div class=\"entry-icon\">\r\n            <el-icon><component :is=\"item.icon\"></component></el-icon>\r\n          </div>\r\n          <div class=\"entry-title\">{{ item.title }}</div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 热门商品 -->\r\n      <div class=\"section-container\">\r\n        <div class=\"section-header\">\r\n          <h2>热门商品</h2>\r\n          <el-button type=\"text\" @click=\"goToCategory('hot')\">查看更多</el-button>\r\n        </div>\r\n        \r\n        <div class=\"product-grid\">\r\n          <div class=\"product-card\" v-for=\"product in hotProducts\" :key=\"product.id\" @click=\"goToProduct(product.id)\">\r\n            <div class=\"product-image\">\r\n              <el-image :src=\"product.image\" fit=\"cover\"></el-image>\r\n              <div class=\"product-tag\" v-if=\"product.tag\">{{ product.tag }}</div>\r\n            </div>\r\n            <div class=\"product-info\">\r\n              <h3 class=\"product-title\">{{ product.name }}</h3>\r\n              <div class=\"product-price\">\r\n                <span class=\"current-price\">¥{{ product.price.toFixed(2) }}</span>\r\n                <span class=\"original-price\" v-if=\"product.originalPrice\">¥{{ product.originalPrice.toFixed(2) }}</span>\r\n              </div>\r\n              <div class=\"product-footer\">\r\n                <div class=\"sold-count\">已售 {{ product.soldCount }}</div>\r\n                <el-button \r\n                  type=\"primary\" \r\n                  size=\"small\" \r\n                  circle\r\n                  @click.stop=\"addToCart(product)\">\r\n                  <el-icon><ShoppingCart /></el-icon>\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 推荐专题 -->\r\n      <div class=\"feature-section\">\r\n        <div class=\"feature-header\">\r\n          <h2>精选专题</h2>\r\n        </div>\r\n        \r\n        <div class=\"feature-container\">\r\n          <div class=\"feature-item\" v-for=\"(feature, index) in features\" :key=\"index\" @click=\"goToFeature(feature.id)\">\r\n            <div class=\"feature-image\">\r\n              <el-image :src=\"feature.image\" fit=\"cover\"></el-image>\r\n            </div>\r\n            <div class=\"feature-content\">\r\n              <h3>{{ feature.title }}</h3>\r\n              <p>{{ feature.desc }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 新品上市 -->\r\n      <div class=\"section-container\">\r\n        <div class=\"section-header\">\r\n          <h2>新品上市</h2>\r\n          <el-button type=\"text\" @click=\"goToCategory('new')\">查看更多</el-button>\r\n        </div>\r\n        \r\n        <div class=\"product-grid\">\r\n          <div class=\"product-card\" v-for=\"product in newProducts\" :key=\"product.id\" @click=\"goToProduct(product.id)\">\r\n            <div class=\"product-image\">\r\n              <el-image :src=\"product.image\" fit=\"cover\"></el-image>\r\n              <div class=\"product-tag new-tag\" v-if=\"product.tag\">{{ product.tag }}</div>\r\n            </div>\r\n            <div class=\"product-info\">\r\n              <h3 class=\"product-title\">{{ product.name }}</h3>\r\n              <div class=\"product-price\">\r\n                <span class=\"current-price\">¥{{ product.price.toFixed(2) }}</span>\r\n                <span class=\"original-price\" v-if=\"product.originalPrice\">¥{{ product.originalPrice.toFixed(2) }}</span>\r\n              </div>\r\n              <div class=\"product-footer\">\r\n                <div class=\"sold-count\">已售 {{ product.soldCount }}</div>\r\n                <el-button \r\n                  type=\"primary\" \r\n                  size=\"small\" \r\n                  circle\r\n                  @click.stop=\"addToCart(product)\">\r\n                  <el-icon><ShoppingCart /></el-icon>\r\n                </el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 品牌专区 -->\r\n      <div class=\"brand-section\">\r\n        <div class=\"section-header\">\r\n          <h2>品牌专区</h2>\r\n        </div>\r\n        \r\n        <div class=\"brand-container\">\r\n          <div class=\"brand-item\" v-for=\"brand in brands\" :key=\"brand.id\" @click=\"goToBrand(brand.id)\">\r\n            <el-image :src=\"brand.logo\" fit=\"contain\"></el-image>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    \r\n    <app-footer></app-footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, reactive, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\nimport { ElMessage } from 'element-plus'\r\nimport { \r\n  ShoppingCart, Goods, Present, MagicStick, \r\n  Discount, Collection, Tickets\r\n} from '@element-plus/icons-vue'\r\nimport AppHeader from '@/components/Header.vue'\r\nimport AppFooter from '@/components/Footer.vue'\r\nimport request from '@/utils/request'\r\n\r\nexport default {\r\n  name: 'HomePage',\r\n  components: {\r\n    AppHeader,\r\n    AppFooter,\r\n    ShoppingCart,\r\n    Goods,\r\n    Present,\r\n    MagicStick,\r\n    Discount,\r\n    Collection,\r\n    Tickets\r\n  },\r\n  setup() {\r\n    const router = useRouter()\r\n    const store = useStore()\r\n    \r\n    // 轮播图数据\r\n    const banners = ref([\r\n      {\r\n        id: 1,\r\n        title: '新品上市',\r\n        subtitle: '2023春季新品，限时8折优惠',\r\n        image: 'https://via.placeholder.com/1200x400?text=Spring+Sale',\r\n        link: '/category/new'\r\n      },\r\n      {\r\n        id: 2,\r\n        title: '数码专场',\r\n        subtitle: '精选数码产品，满2000减200',\r\n        image: 'https://via.placeholder.com/1200x400?text=Digital+Products',\r\n        link: '/category/digital'\r\n      },\r\n      {\r\n        id: 3,\r\n        title: '会员专享',\r\n        subtitle: '会员专享价，更有积分双倍',\r\n        image: 'https://via.placeholder.com/1200x400?text=Member+Benefits',\r\n        link: '/member'\r\n      }\r\n    ])\r\n    \r\n    // 快捷入口\r\n    const quickEntries = ref([\r\n      {\r\n        id: 1,\r\n        title: '新品发布',\r\n        icon: 'Present',\r\n        link: '/category/new'\r\n      },\r\n      {\r\n        id: 2,\r\n        title: '热卖商品',\r\n        icon: 'Goods',\r\n        link: '/category/hot'\r\n      },\r\n      {\r\n        id: 3,\r\n        title: '限时特惠',\r\n        icon: 'Discount',\r\n        link: '/category/discount'\r\n      },\r\n      {\r\n        id: 4,\r\n        title: '优选好物',\r\n        icon: 'MagicStick',\r\n        link: '/category/selected'\r\n      },\r\n      {\r\n        id: 5,\r\n        title: '品牌专区',\r\n        icon: 'Collection',\r\n        link: '/brands'\r\n      }\r\n    ])\r\n    \r\n    // 热门商品\r\n    const hotProducts = ref([\r\n      {\r\n        id: 101,\r\n        name: '高性能笔记本电脑',\r\n        price: 5999,\r\n        originalPrice: 6799,\r\n        image: 'https://via.placeholder.com/300x300?text=Laptop',\r\n        soldCount: 1200,\r\n        tag: '热销'\r\n      },\r\n      {\r\n        id: 102,\r\n        name: '无线蓝牙耳机',\r\n        price: 299,\r\n        originalPrice: 399,\r\n        image: 'https://via.placeholder.com/300x300?text=Headphones',\r\n        soldCount: 3500,\r\n        tag: '热销'\r\n      },\r\n      {\r\n        id: 103,\r\n        name: '智能手表',\r\n        price: 1299,\r\n        originalPrice: 1599,\r\n        image: 'https://via.placeholder.com/300x300?text=SmartWatch',\r\n        soldCount: 800,\r\n        tag: '热销'\r\n      },\r\n      {\r\n        id: 104,\r\n        name: '超薄平板电脑',\r\n        price: 2999,\r\n        originalPrice: 3599,\r\n        image: 'https://via.placeholder.com/300x300?text=Tablet',\r\n        soldCount: 600,\r\n        tag: '热销'\r\n      }\r\n    ])\r\n    \r\n    // 新品上市\r\n    const newProducts = ref([\r\n      {\r\n        id: 201,\r\n        name: '智能家居套装',\r\n        price: 899,\r\n        originalPrice: null,\r\n        image: 'https://via.placeholder.com/300x300?text=SmartHome',\r\n        soldCount: 150,\r\n        tag: '新品'\r\n      },\r\n      {\r\n        id: 202,\r\n        name: '便携显示器',\r\n        price: 1499,\r\n        originalPrice: 169