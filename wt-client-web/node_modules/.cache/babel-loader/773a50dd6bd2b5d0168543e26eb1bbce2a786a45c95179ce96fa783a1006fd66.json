n          class=\"brand-card\"\r\n          @click=\"goToBrand(brand.id)\"\r\n        >\r\n          <div class=\"brand-logo\">\r\n            <img :src=\"brand.logo || '/images/placeholder.png'\" :alt=\"brand.name\">\r\n          </div>\r\n          <div class=\"brand-info\">\r\n            <h3 class=\"brand-name\">{{ brand.name }}</h3>\r\n            <p class=\"brand-desc\" v-if=\"brand.description\">{{ brand.description }}</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <div v-else class=\"empty-brands\">\r\n        <el-empty description=\"暂无品牌\" />\r\n      </div>\r\n    </div>\r\n    \r\n    <app-footer></app-footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\nimport AppHeader from '@/components/Header.vue'\r\nimport AppFooter from '@/components/Footer.vue'\r\n\r\nexport default {\r\n  name: 'BrandsPage',\r\n  components: {\r\n    AppHeader,\r\n    AppFooter\r\n  },\r\n  setup() {\r\n    const router = useRouter()\r\n    \r\n    const loading = ref(true)\r\n    const brands = ref([])\r\n    \r\n    // 获取品牌数据\r\n    const fetchBrands = async () => {\r\n      loading.value = true\r\n      try {\r\n        // 模拟数据加载\r\n        await new Promise(resolve => setTimeout(resolve, 500))\r\n        \r\n        // 模拟品牌数据\r\n        brands.value = [\r\n          {\r\n            id: 1,\r\n            name: '深海探索',\r\n            logo: '/images/brands/deep-sea.png',\r\n            description: '专注于深海潜水设备的专业品牌'\r\n          },\r\n          {\r\n            id: 2,\r\n            name: '蓝洋',\r\n            logo: '/images/brands/blue-ocean.png',\r\n            description: '提供全方位海洋运动装备的综合品牌'\r\n          },\r\n          {\r\n            id: 3,\r\n            name: '海岸线',\r\n            logo: '/images/brands/coastline.png',\r\n            description: '专业海滩度假用品品牌'\r\n          },\r\n          {\r\n            id: 4,\r\n            name: '航海者',\r\n            logo: '/images/brands/navigator.png',\r\n            description: '高端航海设备供应商'\r\n          },\r\n          {\r\n            id: 5,\r\n            name: '珊瑚礁',\r\n            logo: '/images/brands/coral-reef.png',\r\n            description: '环保型海洋用品品牌，致力于海洋保护'\r\n          },\r\n          {\r\n            id: 6,\r\n            name: '海浪',\r\n            logo: '/images/brands/sea-wave.png',\r\n            description: '专注冲浪和海滩运动的专业品牌'\r\n          }\r\n        ]\r\n      } catch (error) {\r\n        console.error('获取品牌数据失败:', error)\r\n      } finally {\r\n        loading.value = false\r\n      }\r\n    }\r\n    \r\n    // 跳转到品牌详情\r\n    const goToBrand = (id) => {\r\n      router.push(`/brand/${id}`)\r\n    }\r\n    \r\n    onMounted(() => {\r\n      fetchBrands()\r\n    })\r\n    \r\n    return {\r\n      loading,\r\n      brands,\r\n      goToBrand\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.brands-page {\r\n  min-height: 100vh;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  flex: 1;\r\n}\r\n\r\n.page-title {\r\n  font-size: 28px;\r\n  color: #333;\r\n  margin-bottom: 30px;\r\n  text-align: center;\r\n}\r\n\r\n.loading-container {\r\n  padding: 20px;\r\n}\r\n\r\n.brands-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\r\n  gap: 25px;\r\n}\r\n\r\n.brand-card {\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  box-shadow: 0 2px 12px rgba(0,0,0,0.1);\r\n  transition: transform 0.2s, box-shadow 0.2s;\r\n  cursor: pointer;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.brand-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 8px 16px rgba(0,0,0,0.15);\r\n}\r\n\r\n.brand-logo {\r\n  height: 180px;\r\n  overflow: hidden;\r\n  background-color: #f8f8f8;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 20px;\r\n}\r\n\r\n.brand-logo img {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n  object-fit: contain;\r\n  transition: transform 0.3s;\r\n}\r\n\r\n.brand-card:hover .brand-logo img {\r\n  transform: scale(1.05);\r\n}\r\n\r\n.brand-info {\r\n  padding: 15px;\r\n  flex-grow: 1;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.brand-name {\r\n  font-size: 18px;\r\n  margin-bottom: 10px;\r\n  color: #333;\r\n  text-align: center;\r\n}\r\n\r\n.brand-desc {\r\n  color: #666;\r\n  font-size: 14px;\r\n  flex-grow: 1;\r\n  text-align: center;\r\n}\r\n\r\n.empty-brands {\r\n  padding: 40px 0;\r\n  text-align: center;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  .brands-grid {\r\n    grid-template-columns: repeat(auto-fill, minmax(250px, 1fr));\r\n  }\r\n}\r\n</style> "],"mappings":";;EAEO,SAAM;AAAa;;EAGjB,SAAM;AAAW;;EAL1BA,GAAA;EAQ0B,SAAM;;;EARhCA,GAAA;EAYyC,SAAM;;iBAZ/C;;EAmBe,SAAM;AAAY;iBAnBjC;;EAsBe,SAAM;AAAY;;EACjB,SAAM;AAAY;;EAvBlCA,GAAA;EAwBe,SAAM;;;EAxBrBA,GAAA;EA6BkB,SAAM;;;;;;;uBA3BtBC,mBAAA,CAiCM,OAjCNC,UAiCM,GAhCJC,YAAA,CAAyBC,qBAAA,GAEzBC,mBAAA,CA2BM,OA3BNC,UA2BM,G,0BA1BJD,mBAAA,CAAgC;IAA5B,SAAM;EAAY,GAAC,MAAI,sBAEhBE,MAAA,CAAAC,OAAO,I,cAAlBP,mBAAA,CAEM,OAFNQ,UAEM,GADJN,YAAA,CAAkCO,sBAAA;IAApBC,IAAI,EAAE,CAAC;IAAEC,QAAQ,EAAR;UAGTL,MAAA,CAAAM,MAAM,CAACC,MAAM,Q,cAA7Bb,mBAAA,CAeM,OAfNc,UAeM,I,kBAdJd,mBAAA,CAaMe,SAAA,QA1BdC,WAAA,CAc0BV,MAAA,CAAAM,MAAM,EAdhC,UAciBK,KAAK;yBADdjB,mBAAA,CAaM;MAXHD,GAAG,EAAEkB,KAAK,CAACC,EAAE;MACd,SAAM,YAAY;MACjBC,OAAK,WAALA,OAAKA,CAAAC,MAAA;QAAA,OAAEd,MAAA,CAAAe,SAAS,CAACJ,KAAK,CAACC,EAAE;MAAA;QAE1Bd,mBAAA,CAEM,OAFNkB,UAEM,GADJlB,mBAAA,CAAsE;MAAhEmB,GAAG,EAAEN,KAAK,CAACO,IAAI;MAAgCC,GAAG,EAAER,KAAK,CAACS;4BApB5EC,UAAA,E,GAsBUvB,mBAAA,CAGM,OAHNwB,UAGM,GAFJxB,mBAAA,CAA4C,MAA5CyB,UAA4C,EAAAC,gBAAA,CAAlBb,KAAK,CAACS,IAAI,kBACRT,KAAK,CAACc,WAAW,I,cAA7C/B,mBAAA,CAA0E,KAA1EgC,WAA0E,EAAAF,gBAAA,CAAxBb,KAAK,CAACc,WAAW,oBAxB/EE,mBAAA,e,mBAAAC,UAAA;qDA6BMlC,mBAAA,CAEM,OAFNmC,WAEM,GADJjC,YAAA,CAA+BkC,mBAAA;IAArBL,WAAW,EAAC;EAAM,G,MAIhC7B,YAAA,CAAyBmC,qBAAA,E","ignoreList":[]},"metad